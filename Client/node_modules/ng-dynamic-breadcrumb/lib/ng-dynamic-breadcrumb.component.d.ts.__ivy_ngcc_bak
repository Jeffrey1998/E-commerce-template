import { OnInit } from '@angular/core';
import { ActivatedRoute, Router } from '@angular/router';
import { Breadcrumb } from './breadcrumb.model';
import { NgDynamicBreadcrumbService } from './ng-dynamic-breadcrumb.service';
export declare class NgDynamicBreadcrumbComponent implements OnInit {
    private activatedRoute;
    private router;
    private ngDynamicBreadcrumbService;
    breadcrumb: Breadcrumb[];
    bgColor: string;
    fontSize: string;
    fontColor: string;
    lastLinkColor: string;
    symbol: string;
    params: {
        [key: string]: any;
    };
    constructor(activatedRoute: ActivatedRoute, router: Router, ngDynamicBreadcrumbService: NgDynamicBreadcrumbService);
    ngOnInit(): void;
    breadCrumbData(): void;
    private updateData;
}
